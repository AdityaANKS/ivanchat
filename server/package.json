    "@sentry/node": "^7.70.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@tensorflow/tfjs-node": "^4.11.0",
    "agenda": "^5.0.0",
    "aws-sdk": "^2.1450.0",
    "axios": "^1.5.0",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "compression": "^1.7.4",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.13",
    "mediasoup": "^3.12.13",
