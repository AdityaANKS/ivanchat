{
  "name": "ivan-server",
  "version": "1.0.0",
  "description": "Ivan backend server",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev:server": "nodemon src/index.js",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "start": "node src/index.js",
    "build": "babel src -d dist",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "test": "jest --coverage",
    "test:unit": "jest --testMatch='**/*.unit.test.js'",
    "test:integration": "jest --testMatch='**/*.integration.test.js'",
    "lint:fix": "eslint src/ --fix",
    "migrate": "node migrations/run.js",
    "seed": "node seeds/run.js",
    "docker:build": "docker build -t ivanchat-server .",
    "security:check": "npm audit && snyk test",
    "generate:docs": "apidoc -i src/routes -o docs/api"
  },
  "dependencies": {
    "@azure/cognitiveservices-contentmoderator": "^5.0.1",
    "@elastic/elasticsearch": "^8.9.0",
    "@google-cloud/speech": "^6.0.0",
